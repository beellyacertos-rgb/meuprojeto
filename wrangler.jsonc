{
  "$schema": "node_modules/wrangler/config-schema.json",
  "name": "semijoias-app",
  "compatibility_date": "2026-02-03",
  "pages_build_output_dir": "./dist",
  "compatibility_flags": [
    "nodejs_compat"
  ],
  "d1_databases": [
    {
      "binding": "DB",
      "database_name": "semijoias-production",
      "database_id": "local-db-id"
    }
  ]
}